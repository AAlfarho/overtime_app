<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">

  <%= render 'partials/header' %>
  <div class="row">
    <div class="col-md-12">
      <%= yield %>
    </div>
  </div>
</div>

<%= js add_gritter(flash[:notice], title: 'Overtime Notification', sticky: false, time: 3000)%>
</body>
</html>
